{
  "name": "ro-virtual-scroll",
  "version": "1.0.0",
  "description": "基于 react v16.8+ 的虚拟长列表组件",
  "author": "roollo",
  "main": "src/index.js",
  "scripts": {
    "clean": "del-cli dist",
    "prebuild": "npm run clean",
    "build": "babel src -d dist --copy-files",
    "prepare": "npm run build",
    "prettier:check": "prettier --c \"src/**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "prettier:write": "prettier --config --write \"src/**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "yarn lint:js --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roooollo/ro-virtual-scroll"
  },
  "keywords": [
    "virtual",
    "scroll"
  ],
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "eslint": "7.15.0",
    "prettier": "2.2.1",
    "del": "^6.0.0",
    "del-cli": "^3.0.1"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 11",
    "not IE 10"
  ]
}
