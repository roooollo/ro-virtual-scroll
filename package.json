{
  "name": "ro-virtual-scroll",
  "version": "1.2.6",
  "description": "Virtual long list component based on react v16.8+ (support variable height)",
  "author": "roollo",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "del-cli dist",
    "build": "babel src -d dist --copy-files",
    "prettier:check": "prettier --c \"src/**/*.{js,ts,md,json,css}\"",
    "prettier:write": "prettier --config --write \"src/**/*.{js,ts,md,json,css}\"",
    "lint:js": "eslint --cache --ext .js,.ts ./src --fix",
    "lint:style": "stylelint \"src/**/*.css\" --fix",
    "lint": "npm-run-all -l -p \"lint:**\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roooollo/ro-virtual-scroll"
  },
  "keywords": [
    "virtual",
    "scroll",
    "react"
  ],
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime-corejs3": "^7.12.1",
    "intersection-observer": "^0.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.1",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "del-cli": "^3.0.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 11",
    "not IE 10"
  ]
}
